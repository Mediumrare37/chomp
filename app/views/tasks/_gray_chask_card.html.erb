<div class="gray-chask-card" data-controller="expand-card">
  <div class="subshow-header">
    <div class="subshow-title">
      <p>Hidden Task</p>
    </div>
  </div>
  <div class="subshow-subheader">
    <div class="chask-status">
      <%= simple_form_for chask, url: queued_chask_path(chask) do |f| %>
        <%= f.input :status, as: :hidden, input_html: { value: 'queued'} %>
        <%= f.submit 'Click to Reveal', class: "recover-button" %>
      <% end %>
    </div>
  </div>
</div>
<div class="subchasks-section d-none" data-expand-card-target="chasks" >
  <%= render 'tasks/subchask_card', chask: chask %>
</div>
</div>
